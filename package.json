{
  "name": "rewritten",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "concurrently \"next start --port 3000\" \"gunicorn --bind 0.0.0.0:5001 app:app\"",
    "flask": "python -m flask run",
    "dev:all": "concurrently \"npm run dev\" \"npm run flask\"",
    "lint": "next lint",
    "setup": "npm install && python -m pip install -r requirements.txt",
    "init-db": "python -m flask init-db"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.70.0",
    "axios": "^1.8.4",
    "next": "15.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
} 
